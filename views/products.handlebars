<section class="grid-box">
    {{#each productData}}
        <div class="grid-product">
            <a href="/shopping/add/{{this.id}}"><img src="/img/{{this.img}}" alt="Image Not Available"/></a>            
            <p>
                <b>Name : {{this.name}}</b>
            </p>
            <p>
                Price: {{this.price}}
            </p>
            <p>
                Description: {{this.description}}
            </p>
        </div>
    {{/each}}
</section>